{
  "name": "My third extension",
  "version": "0.1",
  "manifest_version": 2,

  "description": "今いるページの背景色を変える",

  "permissions": [
    "tabs", "http://*/*"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "url 取得"
  }
}
